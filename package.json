  "scripts": {
    "dev": "webpack serve --config ./webpack.demo.config.js",
    "clear-dists": "rimraf dist",
    "prebuild:lib": "npm run clear-dists",
    "build:lib:watch": "NODE_ENV=production rollup --config ./internals/rollup/rollup.prod.config.js --watch",
    "build:lib": "NODE_ENV=production rollup --config ./internals/rollup/rollup.prod.config.js",
    "build:lib:unminified": "NODE_ENV=development rollup --config ./internals/rollup/rollup.storybook.config.js",
    "build": "npm run build:lib; npm run build:lib:unminified",
    "lint": "npm run typecheck && npm run lint:eslint",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx --fix .",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run test:clean && npm run lint",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=17.x.x",
    "react-dom": ">=17.x.x"
  },
